<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="10"
    android:orientation="vertical"
    tools:context="com.sidoso.paciente.AssociatedActivity">

    <include 
        android:id="@+id/tollbarAssociated"
        layout="@layout/toolbar" />

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        tools:context="com.sidoso.paciente.AssociatedActivity" />

    <TextView
        android:id="@+id/tv_associated_contact"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:textStyle="italic"
        android:layout_marginLeft="@dimen/margin_form_item"
        android:layout_marginRight="@dimen/margin_form_item"
        />

</LinearLayout>